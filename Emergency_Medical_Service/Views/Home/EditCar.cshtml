@model EMS.Lib.Models.CarModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div>
    <form asp-action="EditCar">
        @Html.HiddenFor(x=>x.CarId)
        <input type="text" value="@Model.Name" asp-for="Name"/>
        <input type="text" value="@Model.Plate" asp-for="Plate"/>
        <button type="submit">Save</button>
    </form>
</div>
